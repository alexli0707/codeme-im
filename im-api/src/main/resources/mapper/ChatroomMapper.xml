<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.codeme.im.imapi.mapper.ChatroomMapper">
    <update id="increaseRoomMemberCount">
        update chatroom set current_count=current_count+#{increaseCount} where id = #{id}
    </update>

</mapper>
